[
  {
    "group": "Font / Zoom",
    "label": "Zoom in (global)",
    "key": "ctrl+-",
    "command": "workbench.action.zoomIn"
  },
  {
    "group": "Font / Zoom",
    "label": "Zoom out (global)",
    "key": "ctrl+-",
    "command": "workbench.action.zoomOut"
  },
  {
    "group": "Font / Zoom",
    "label": "Reset zoom (global)",
    "key": "ctrl+0",
    "command": "workbench.action.zoomReset"
  },
  {
    "group": "Font / Zoom",
    "label": "Zoom in editor font",
    "key": "cmd+=",
    "command": "editor.action.fontZoomIn",
    "when": "editorTextFocus"
  },
  {
    "group": "Font / Zoom",
    "label": "Zoom out editor font",
    "key": "cmd+-",
    "command": "editor.action.fontZoomOut",
    "when": "editorTextFocus"
  },
  {
    "group": "Font / Zoom",
    "label": "Reset editor font zoom",
    "key": "cmd+0",
    "command": "editor.action.fontZoomReset",
    "when": "editorTextFocus"
  },
  {
    "group": "Editor Tabs",
    "label": "Open editor tab 1",
    "key": "cmd+1",
    "command": "workbench.action.openEditorAtIndex1"
  },
  {
    "group": "Editor Tabs",
    "label": "Open editor tab 2",
    "key": "cmd+2",
    "command": "workbench.action.openEditorAtIndex2"
  },
  {
    "group": "Editor Tabs",
    "label": "Open editor tab 3",
    "key": "cmd+3",
    "command": "workbench.action.openEditorAtIndex3"
  },
  {
    "group": "Editor Tabs",
    "label": "Open editor tab 4",
    "key": "cmd+4",
    "command": "workbench.action.openEditorAtIndex4"
  },
  {
    "group": "Editor Tabs",
    "label": "Open editor tab 5",
    "key": "cmd+5",
    "command": "workbench.action.openEditorAtIndex5"
  },
  {
    "group": "Editor Tabs",
    "label": "Previous editor",
    "key": "cmd+[",
    "command": "workbench.action.previousEditor"
  },
  {
    "group": "Editor Tabs",
    "label": "Next editor",
    "key": "cmd+]",
    "command": "workbench.action.nextEditor"
  },
  {
    "group": "Editor Tabs",
    "label": "Show all opened editors",
    "key": "cmd+p",
    "command": "workbench.action.showAllEditors"
  },
  {
    "group": "Editor Groups",
    "label": "Move editor to next group",
    "key": "ctrl+cmd+right",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "group": "Editor Groups",
    "label": "Move editor to previous group",
    "key": "ctrl+cmd+left",
    "command": "workbench.action.moveEditorToPreviousGroup"
  },
  {
    "group": "Editor Groups",
    "label": "Focus next editor group",
    "key": "ctrl+tab",
    "command": "workbench.action.focusNextGroup"
  },
  {
    "group": "Editor Groups",
    "label": "Focus previous editor group",
    "key": "ctrl+shift+tab",
    "command": "workbench.action.focusPreviousGroup"
  },
  {
    "group": "Sidebar / Explorer",
    "label": "Toggle sidebar visibility",
    "key": "cmd+e",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "group": "Sidebar / Explorer",
    "label": "Focus explorer view",
    "key": "cmd+shift+e",
    "command": "workbench.view.explorer"
  },
  {
    "group": "Sidebar / Explorer",
    "label": "Open selected file/folder",
    "key": "enter",
    "command": "list.select",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "group": "Sidebar / Explorer",
    "label": "Create new file",
    "key": "cmd+n",
    "command": "explorer.newFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "group": "Sidebar / Explorer",
    "label": "Rename file",
    "key": "cmd+r",
    "command": "renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "group": "Sidebar / Explorer",
    "label": "Delete file",
    "key": "cmd+backspace",
    "command": "deleteFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "group": "Terminal",
    "label": "Focus terminal",
    "key": "ctrl+t",
    "command": "workbench.action.terminal.focus",
    "when": "!terminalFocus"
  },
  {
    "group": "Terminal",
    "label": "Focus editor from terminal",
    "key": "ctrl+t",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  {
    "group": "Terminal",
    "label": "New terminal",
    "key": "cmd+shift+t",
    "command": "workbench.action.terminal.new"
  },
  {
    "group": "Cursor Movement",
    "label": "Move cursor to line start",
    "key": "cmd+left",
    "command": "cursorHome",
    "when": "editorTextFocus"
  },
  {
    "group": "Cursor Movement",
    "label": "Move cursor to line end",
    "key": "cmd+right",
    "command": "cursorEnd",
    "when": "editorTextFocus"
  },
  {
    "group": "Cursor Movement",
    "label": "Move cursor to top of file",
    "key": "cmd+up",
    "command": "cursorTop",
    "when": "editorTextFocus"
  },
  {
    "group": "Cursor Movement",
    "label": "Move cursor to bottom of file",
    "key": "cmd+down",
    "command": "cursorBottom",
    "when": "editorTextFocus"
  },
  {
    "group": "Cursor Movement",
    "label": "Move cursor to previous word",
    "key": "alt+left",
    "command": "cursorWordStartLeft",
    "when": "editorTextFocus"
  },
  {
    "group": "Cursor Movement",
    "label": "Move cursor to next word",
    "key": "alt+right",
    "command": "cursorWordEndRight",
    "when": "editorTextFocus"
  },
  {
    "group": "Cursor Movement",
    "label": "Move cursor down 5 lines",
    "key": "ctrl+alt+down",
    "command": "cursorMove",
    "when": "editorTextFocus",
    "args": { "to": "down", "by": "line", "value": 5 }
  },
  {
    "group": "Cursor Movement",
    "label": "Move cursor up 5 lines",
    "key": "ctrl+alt+up",
    "command": "cursorMove",
    "when": "editorTextFocus",
    "args": { "to": "up", "by": "line", "value": 5 }
  },
  {
    "group": "Editing",
    "label": "Insert line below",
    "key": "cmd+enter",
    "command": "editor.action.insertLineAfter",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "group": "Editing",
    "label": "Insert line above",
    "key": "cmd+shift+enter",
    "command": "editor.action.insertLineBefore",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "group": "Editing",
    "label": "Format document",
    "key": "shift+alt+f",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
  }
]
